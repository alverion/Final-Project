<!DOCTYPE html>
<html>
<head>
  <title>CliqSynq</title>
  <%= stylesheet_link_tag    "application" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
<div id="login">
	<%if session[:member_id] %>
		Welcome <%= Member.find_by_id(session[:member_id]).first_name %> | <%= link_to 'logout', logout_path, :method => :delete %>	
	<% end %>
</div>
<%= yield %>

</body>
</html>
